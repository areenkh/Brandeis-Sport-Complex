<h2>Users Table</h2>
<table class="table styled-table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Zip Code</th>
      <th>Role</th>
      <th>Graduation Year</th>
      <th>Major</th>
      <th>Job</th>
      <th>Company</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Bio</th>
      <th>Interests</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
        <td>
            <a href="<%= `/users/${user._id}` %>"> <%= user.name %> </a>
        </td>
        <td><%= user.email %></td>
        <td><%= user.zipCode %></td>
        <td><%= user.role %></td>
        <td><%= user.graduationYear %></td>
        <td><%= user.major %></td>
        <td><%= user.job %></td>
        <td><%= user.company %></td>
        <td><%= user.city %></td>
        <td><%= user.state %></td>
        <td><%= user.country %></td>
        <td><%= user.bio %></td>
        <td><%= user.interests %></td>
      <td>
        <a href="<%=`/users/${user._id}/edit` %>"> Edit </a>
      </td>
      <td>
      <!--not being able to delete user when not logged in -->
        <a
          href="<%= `users/${user._id}/delete?_method=DELETE` %>"
          onclick="return confirm('Are you sure you want to delete this record?')"
          >Delete</a
        >
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
